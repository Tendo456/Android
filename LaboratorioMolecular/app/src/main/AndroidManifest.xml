<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.laboratoriomolecular">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LaboratorioMolecular"
        android:usesCleartextTraffic="true">
        <activity android:name=".Actividades.PlacasDetails"
            android:label="@string/Nueva_placaD_title"
            android:screenOrientation="portrait"/>
        <activity android:name=".Actividades.nueva_placa"
            android:label="@string/Nueva_placa_title"
            android:screenOrientation="portrait"/>
        <activity android:name=".Actividades.RecepcionDetails"
            android:label="@string/RecepcionD_title"
            android:screenOrientation="portrait"/>
        <activity android:name=".Actividades.resultados"
            android:label="@string/Resultados_title"
            android:screenOrientation="portrait"/>
        <activity android:name=".Actividades.amplificacion"
            android:label="@string/Amplificacion_title"
            android:screenOrientation="portrait"/>
        <activity android:name=".Actividades.area_limpia"
            android:label="@string/Area_title"
            android:screenOrientation="portrait"/>
        <activity android:name=".Actividades.extraccion"
            android:label="@string/Extraccion_title"
            android:screenOrientation="portrait"/>
        <activity android:name=".Actividades.alicuotado"
            android:label="@string/Alicuotado_title"
            android:screenOrientation="portrait"/>
        <activity android:name=".Actividades.recepcion"
            android:label="@string/Recepcion_title"
            android:screenOrientation="portrait"/>
        <activity android:name=".Actividades.Formularios"
            android:label="@string/Formularios_title"
            android:screenOrientation="portrait"/>
        <activity android:name=".Actividades.MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.LaboratorioMolecularUno">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>