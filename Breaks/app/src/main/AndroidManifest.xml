<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.breaks">

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Breaks"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".actividades.Breaks"
            android:exported="false" />
        <activity
            android:name=".actividades.orientation"
            android:screenOrientation="fullSensor"
            tools:replace="screenOrientation" />
        <activity
            android:name=".actividades.Personal_add"
            android:exported="false" />
        <activity
            android:name=".actividades.PersonalDetails"
            android:exported="false" />
        <activity
            android:name=".actividades.Personal"
            android:exported="false" />
        <activity
            android:name=".actividades.Programacion_add"
            android:exported="false" />
        <activity
            android:name=".actividades.ProgramacionDetails"
            android:exported="false" />
        <activity
            android:name=".actividades.Programacion"
            android:exported="false" />
        <activity
            android:name=".actividades.Stock_add"
            android:exported="false" />
        <activity
            android:name=".actividades.StockDetails"
            android:exported="false" />
        <activity
            android:name=".actividades.Menu"
            android:exported="false" />
        <activity
            android:name=".actividades.Stock"
            android:exported="false" />
        <activity
            android:name=".actividades.MarcasDetails"
            android:exported="false" />
        <activity
            android:name=".actividades.Marcas_add"
            android:exported="false" />
        <activity
            android:name=".actividades.Marcas"
            android:exported="false" />
        <activity
            android:name=".actividades.InicioActivity"
            android:exported="false" />
        <activity
            android:name=".actividades.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>